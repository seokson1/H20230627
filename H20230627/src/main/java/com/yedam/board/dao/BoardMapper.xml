  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.board.dao.BoardMapper">
  <select id="listBoard">
  	select * from tbl_board;
  </select>
  <select id="getBoard" parameterType="long">
  	select * from tbl_board where brdNo = #{brd_no};
  </select>

  
  
